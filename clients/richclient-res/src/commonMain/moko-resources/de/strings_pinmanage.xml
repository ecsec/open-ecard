	<!-- Resume PIN -->
	<string name="pinmanage_npa_pin_suspend_title">PIN entsperren</string>
	<string name="pinmanage_npa_pin_suspend_description">
		Ihre PIN wurde auf Grund zweier Falscheingaben gesperrt.
		Um die PIN zu entsperren, müssen Sie die Kartenkennnummer (CAN) und die aktuellen PIN eingeben.
		Die CAN finden Sie auf der Vorderseite Ihres Personalausweises.\n
		Bitte beachten Sie: Wenn die eingegebene PIN nicht korrekt ist, wird die Karte gesperrt und muss mit dem PUK
		entsperrt werden.
	</string>

	<string name="pinmanage_npa_pin_suspend_can_field_initial">CAN eingeben</string>
	<string name="pinmanage_npa_pin_suspend_pin_field_initial">PIN eingeben</string>
	<string name="pinmanage_npa_pin_suspend_error_empty_can">Bitte geben Sie die CAN ein.</string>
	<string name="pinmanage_npa_pin_suspend_error_invalid_can">Die CAN muss 5 oder 6 Ziffern lang sein.</string>
	<string name="pinmanage_npa_pin_suspend_error_empty_pin">Bitte geben Sie Ihre PIN ein.</string>
	<string name="pinmanage_npa_pin_suspend_error_invalid_pin">Die PIN muss 5 oder 6 Ziffern lang sein.</string>

