	<string name="pinmanage_npa_pin_change_error_invalid_length">Jede PIN muss 5 oder 6 Ziffern lang sein.</string>
	<string name="pinmanage_npa_pin_change_error_mismatch">Die Eingaben der neuen PIN stimmen nicht überein.</string>

	<!-- Unblock PIN -->
