<resources>
	<string name="pinmanage_name">PIN Management</string>
	<string name="pinmanage_select_card">Select a card</string>

	<!-- Resume PIN -->
	<string name="pinmanage_npa_pin_suspend_title">Resume PIN</string>
	<string name="pinmanage_npa_pin_suspend_description">
		Your PIN is suspended because you have entered the PIN two times wrong.
		To reset the PIN status, you need to enter the card access number (CAN) and the current PIN.
		You will find the CAN right on the front of your ID card.\n
		Note that if the provided PIN is not correct, the card will be blocked an needs to be unblocked with the PUK.
	</string>
	<string name="pinmanage_npa_pin_suspend_can_field_initial">Enter CAN</string>
	<string name="pinmanage_npa_pin_suspend_pin_field_initial">Enter PIN</string>
	<string name="pinmanage_npa_pin_suspend_error_empty_can">Please enter the CAN.</string>
	<string name="pinmanage_npa_pin_suspend_error_invalid_can">The CAN must be 5 or 6 digits long.</string>
	<string name="pinmanage_npa_pin_suspend_error_empty_pin">Please enter your PIN.</string>
	<string name="pinmanage_npa_pin_suspend_error_invalid_pin">The PIN must be 5 or 6 digits long.</string>

	<!-- Change PIN -->
	<string name="pinmanage_npa_pin_change_title">Change PIN</string>
	<string name="pinmanage_npa_pin_change_description">You can change your PIN here.</string>
	<string name="pinmanage_npa_pin_change_old_field_initial">Current PIN</string>
	<string name="pinmanage_npa_pin_change_new_field_initial">New PIN</string>
	<string name="pinmanage_npa_pin_change_repeat_field_initial">Repeat new PIN</string>
	<string name="pinmanage_npa_pin_change_error_empty_fields">All fields must be filled in.</string>
	<string name="pinmanage_npa_pin_change_error_invalid_length">Each PIN must be 5 or 6 digits long.</string>
	<string name="pinmanage_npa_pin_change_error_mismatch">The new PIN entries do not match.</string>

	<!-- Unblock PIN -->
	<string name="pinmanage_npa_pin_unblock_title">Unblock PIN</string>
	<string name="pinmanage_npa_pin_unblock_description">Your PIN is blocked and must be unblocked using your personal
		unblocking key (PUK).
	</string>
	<string name="pinmanage_npa_pin_unblock_puk_field_initial">PUK</string>
	<string name="pinmanage_npa_pin_unblock_error_empty_puk">Please enter the PUK.</string>
	<string name="pinmanage_npa_pin_unblock_error_invalid_puk">The PUK must be exactly 10 digits long.</string>

